<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  

    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  

    xmlns:p="http://www.springframework.org/schema/p"  

    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
    <!-- default example (autowire="no") -->
    <bean id="driver" class="com.bridgelabzSprings.NoAutowire.Driver" autowire="no">
        <property name="license" ref="license"/>
    </bean>
 
    <bean id="license" class="com.bridgelabzSprings.NoAutowire.License" >
        <property name="number" value="1234CD"/>
    </bean>
 
</beans>